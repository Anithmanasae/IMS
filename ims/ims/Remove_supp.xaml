<Window x:Class="Inventory_management_system.Remove_supp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Remove_supp" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="423" Width="745">
    <Window.Background>
        <ImageBrush>
            <ImageBrush.ImageSource>
                <BitmapImage UriSource="Images\Admin.jpg"/>
            </ImageBrush.ImageSource>
        </ImageBrush>
    </Window.Background>
    <Grid >
        <Label Content="REMOVE SUPPLER" Height="28" HorizontalAlignment="Left" Margin="281,0,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="SUPPLER ID" Height="28" HorizontalAlignment="Left" Margin="107,106,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="231,106,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
        <Button Content="REMOVE" Height="23" HorizontalAlignment="Left" Margin="231,229,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="HOME" Height="23" HorizontalAlignment="Left" Margin="393,229,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
    </Grid>
</Window>
